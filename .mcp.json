{
  "mcpServers": {
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    },
    "atlassian": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/inspector", "mcp-remote", "https://mcp.atlassian.com/v1/sse"]
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@executeautomation/playwright-mcp-server"]
    }
  }
}
